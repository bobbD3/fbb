<!DOCTYPE html>
<html>

<head>
  <!-- Importanat meta tags -->
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta http-equiv="refresh" content="3600">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="Description"
    content="functional bodies, функционални тела, силови тренировки, функционални тренировки, Холистични тренировки">
  <meta name="keywords"
    contnet="сила, здраве, функционални тела, functional bodies, болки в колена, Холистични тренировки">

  <title>Functional bodies</title>

  <!-- FavIcon -->
  <link rel="shortcut icon" href="/img/fb.jpg" height="5">
  <!-- font awesome-->
  <link href="https://fonts.googleapis.com/css?family=Oswald&display=swap" rel="stylesheet">
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css"
    integrity="sha384-oS3vJWv+0UjzBfQzYUhtDYW+Pj2yciDJxpsK1OYPAYjqT085Qq/1cq5FLXAZQ7Ay" crossorigin="anonymous">
  <link rel="stylesheet" href="./fonts/KelsonSans-Bold.ttf" crossorigin="anonymous">
  <!-- Bootstrap -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css"
    integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
  <!-- Animate CSS -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.7.2/animate.min.css">
  <!-- Customer CCS -->
  <link type="text/css" rel="stylesheet" href="/css/styles.css">
  <link type="text/css" rel="stylesheet" href="/css/responsive.css">


  <style>
    a:link {
      text-decoration: none;
    }

    a:visited {
      text-decoration: none;
    }

    a:hover {
      text-decoration: underline;
    }

    a:active {
      text-decoration: underline;
    }

    a:hover {
      text-decoration: none;
    }
  </style>

</head>

<body>
  <div id="preloader">
    <div id="status">&nbsp;</div>
  </div>

  <nav class="navbar navbar-expand-lg fixed-top shadow ">
    <div class="container">
      <a class="navbar-brand" href="/">
        <img src="/img/logo.png" alt="logo" width="75">
      </a>
      <button class="navbar-toggler " type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup"
        aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon "> <i class="fas fa-bars"></i> </span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
        <ul class="navbar-nav">
          <li class="nav-item active">
            <a class="nav-link" href="/#info">За нас</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/#services">Услуги</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/#portfolio">Прогресии</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/#blog">Blog</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/#contact">Контакти</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">Магазин</a>
          </li>
          <% if(!currentUser){ %>
          <li class="nav-item flex-wrap: wrap"><a class="nav-link" href="/login">Login</a></li>
          <li class="nav-item flex-wrap: wrap"><a class="nav-link" href="/register">Sign Up</a></li>
          <% } else { %>
          <li class="nav-item flex-wrap: wrap"><a class="nav-link" href="#">Signed In As <%= currentUser.username %></a>
          </li>
          <li class="nav-item flex-wrap: wrap"><a class="nav-link" href="/logout">Logout</a></li>
          <% } %>
        </ul>
        <!-- <div class="form-inline my-2 my-lg-0">
                  Navbar text with an inline element
                </div> -->
      </div>
    </div>
  </nav><br><br><br><br>

  <div class="container">
    <% if (error && error.length > 0) { %>
    <div class="alert alert-danger" role="alert">
        <%= error %>
    </div>
    <% } %>
    <% if (success && success.length > 0) { %>
    <div class="alert alert-primary" role="alert">
         <%= success %>
    </div>
    <% } %>
  </div>



  <section id="SideNav">
    <div id="mySidenav" class="sidenav">
      <a href="https://www.facebook.com/functionalbodiesbg/" id="FaceBook" target="_parent"><i
          class="fab fa-facebook-square side-icon"></i></a>
      <a href="https://www.instagram.com/functionalbodiesbg/" id="Instagram" target="_parent"><i
          class="fab fa-instagram side-icon"></i>
      </a>
      <a href="https://www.youtube.com/channel/UC2WmMlDwRFi8rYCZRGkhG1g" id="YouTube" target="_parent"><i
          class="fab fa-youtube side-icon" aria-hidden="true"></i>

      </a>
      <a href="/#contact" id="PhoneContact"><i class="fa fa-phone-square side-icon"></i>
      </a>
    </div>
  </section>